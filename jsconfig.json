{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6"
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"],
  "paths": {
    "@components/*": ["src/components/*"],
    "@assets/*": ["src/assets/*"],
    "@stores/*": ["src/stores/*"],
    "@constants/*": ["src/constants/*"],
    "@router/*": ["src/router/*"],
    "@utils/*": ["src/utils/*"]
  }
}
